{
  "name": "route-peek",
  "description": "Library for route pathname parsing",
  "repository": {
    "type": "git",
    "url": "https://github.com/stenin-nikita/route-peek"
  },
  "type": "module",
  "version": "0.0.8",
  "keywords": [
    "path",
    "pattern",
    "regexp",
    "route"
  ],
  "files": [
    "lib"
  ],
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs"
    }
  },
  "scripts": {
    "build": "unbuild",
    "test": "vitest",
    "test:coverage": "vitest run --coverage --coverage.include=src"
  },
  "author": "Nikita Stenin <stenin.nikita@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@vitest/coverage-v8": "^2.1.3",
    "prettier": "^3.3.3",
    "recheck": "^4.4.5",
    "typescript": "^5.6.3",
    "unbuild": "^2.0.0",
    "vitest": "^2.1.3"
  }
}
